{
    "name": "pink-pepper-gin",
    "displayName": "Gin Syntax Highlighter - PinkPepperGin",
    "description": "Add synthax highligh for gin config files",
    "version": "0.4.0",
    "engines": {
        "vscode": "^1.67.0"
    },
    "repository": "https://github.com/NielsPichon/gin-VSCode-Extension.git",
    "publisher": "NielsPichon",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "gin",
                "aliases": [
                    "gin",
                    "gin"
                ],
                "extensions": [
                    ".gin"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gin",
                "scopeName": "source.gin",
                "path": "./syntaxes/gin.tmLanguage.json"
            }
        ]
    }
}